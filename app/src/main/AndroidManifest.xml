<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rui.yunifang">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.CALL_PHONE"/>

    <application
        android:name=".application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="design_width"
            android:value="768" />
        <meta-data
            android:name="design_height"
            android:value="1280" />

        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.LaunchActivity"></activity>
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.AllGoodsActivity" />
        <activity android:name=".activity.DynamicActivity" />
        <activity android:name=".activity.MoreShopActivity" />
        <activity android:name=".activity.SubjectActivity" />
        <activity android:name=".activity.GoodsActivity" />
        <activity android:name=".activity.SettingsActivity" />
        <activity android:name=".activity.TestActivity" />
        <activity android:name=".activity.SoatMaskActivity">

        </activity>
    </application>

</manifest>